<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cluster.entity">
    <class name="Host" table="host">
        <id name="name" column="name" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <bag name="engineList" cascade="save-update">
            <key column="role_h" not-null="true" />
            <one-to-many class="EngineRole" />
        </bag>
    </class>
</hibernate-mapping>